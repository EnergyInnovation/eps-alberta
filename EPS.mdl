	GET DIRECT DATA(
	   'InputData/elec/EIaE/EIaE-BEE.csv',
	   ',',
	   '1',
	   'B2'
	)
	~	MW*hour
	~		|

Imported Electricity[Electricity Source]=
	EIaE BAU Imported Electricity[Electricity Source]
	* (1 + Percent Change in Electricity Imports This Year)
	~	MW*hour
	~		|

