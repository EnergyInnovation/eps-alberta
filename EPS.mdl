BAU New Elec Output Allocated Freely[Electricity Source]=
	ALLOCATE AVAILABLE(
	  BAU Max New Elec Output Buildable after RPS[Electricity Source],
	  BAU New Elec Output Priority Profile[Electricity Source,ptype],
	  BAU Elec to Allocate Without RPS
	)
	~	MW*hour
	~		|

BAU New Elec Output Allocated Under RPS[Electricity Source]=
	ALLOCATE AVAILABLE(
	   BAU Max RPS Qualifying Electric Output Buildable[Electricity Source],
	   BAU New Elec Output Priority Profile[Electricity Source, ptype],
	   BAU Additional Potential Output Needed to Meet RPS
	)
	~	MW*hour
	~		|

BAU Retiring Generation Capacity by Quality Level[Electricity Source,preexisting retiring\
		]=
	MIN(
	
	  BCR BAU Capacity Retirements[Electricity Source] / One Year,
	
	  BAU Last Year Electricity Generation Capacity[Electricity Source,preexisting retiring\
		] / One Year
	) ~~|
BAU Retiring Generation Capacity by Quality Level[Electricity Source,preexisting nonretiring\
		]=
	MAX(
	
	  0,
	
	  MIN(
	
	    (BCR BAU Capacity Retirements[Electricity Source]
	      - BAU Last Year Electricity Generation Capacity[Electricity Source,preexisting retiring\
		])
	    / One Year, 
	
	    BAU Last Year Electricity Generation Capacity[Electricity Source,preexisting nonretiring\
		] / One Year
	  )
	) ~~|
BAU Retiring Generation Capacity by Quality Level[Electricity Source,newly built]=
	MAX(
	
	  0,
	
	  MIN(
	
	    (BCR BAU Capacity Retirements[Electricity Source]
	      - BAU Last Year Electricity Generation Capacity[Electricity Source,preexisting retiring\
		]
	      - BAU Last Year Electricity Generation Capacity[Electricity Source,preexisting nonretiring\
		])
	    / One Year,
	
	    BAU Last Year Electricity Generation Capacity[Electricity Source,newly built] / \
		One Year
	  )
	)
	~	MW/Year
	~		|

