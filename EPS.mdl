	    SUM(Vehicles[Vehicle Type,Cargo Type,Vehicle Technology!])
	  )
	~	thing*miles
	~		|

Perc Change in Transportation Fuel Cost per Unit Energy[Transportation Fuel]=
	QUANTUM(
	  ZIDZ(
	    Transportation Fuel Cost per Unit Energy[Transportation Fuel]
	    - BAU Transportation Fuel Cost per Unit Energy[Transportation Fuel],
	
	    BAU Transportation Fuel Cost per Unit Energy[Transportation Fuel]
	  ),
	
	  Rounding Error Quantization Size
	)
	~	Dmnl
	~		|

Rounding Error Quantization Size=
	1e-005
	~	Dmnl
	~		|

Vehicle Technology Priority Profile[Vehicle Type,Cargo Type,Vehicle Technology,ptype]\
		=
	13 {ptype: normal distribution, integer-only allocation} ~~|
Vehicle Technology Priority Profile[Vehicle Type,Cargo Type,Vehicle Technology,ppriority\
		]=
	-NPV of Lifetime Vehicle Cost[Vehicle Type,Cargo Type,Vehicle Technology]
	* Vehicle Priority Profile Unit Correction Factor
	
	{ppriority: the center point of the normal distribution} ~~|
Vehicle Technology Priority Profile[Vehicle Type,Cargo Type,Vehicle Technology,pwidth\
		]=
	MAX(
	  SDoVPbT Standard Dev of Vehicle Prices by Technology[Vehicle Type,Cargo Type,Vehicle Technology\
		]
	  * Vehicle Priority Profile Unit Correction Factor,
	
	  0
	)
	
	
	{pwidth: the standard deviation of the normal distribution} ~~|
Vehicle Technology Priority Profile[Vehicle Type,Cargo Type,Vehicle Technology,pextra\
		]=
	0 {pextra: this value is ignored when using normal distribution curve type}
	~	Dmnl
	~		|

Vehicle Priority Profile Unit Correction Factor=
	1
	~	vehicles/$
	~		|

NPV of Lifetime Fuel Expenditures for a Vehicle Sold This Year[Vehicle Type,Cargo Type\
		,Vehicle Technology]=
	Annual Fuel Expenditures for a Vehicle Sold This Year[Vehicle Type,Cargo Type,Vehicle Technology\
		]
	* (
	    (1 - (1 + VBDR Vehicle Buyer Discount Rate[Vehicle Type]) ^ (- AVL Avg Vehicle Lifetime\
		[Vehicle Type]/One Year))
	    / VBDR Vehicle Buyer Discount Rate[Vehicle Type]
	  )
	~	$/vehicle
	~		|

Last Year Weighted Contribution of All Vehicles to Fleet Avg Fuel Economy[Vehicle Type\
		,Cargo Type,Vehicle Technology]= INTEG (
	Weighted Contribution of New Vehicles to Fleet Avg Fuel Economy[Vehicle Type,Cargo Type\
		,Vehicle Technology]-Weighted Contribution of Retiring Vehicles to Fleet Avg Fuel Economy\
		[Vehicle Type,Cargo Type,Vehicle Technology],
		SYVbT Start Year Vehicles by Technology[Vehicle Type,Cargo Type,Vehicle Technology]
		* SYFAFE Start Year Fleet Avg Fuel Economy[Vehicle Type,Cargo Type,Vehicle Technology\
		])
	~	thing*miles*vehicles/BTU
	~		|

