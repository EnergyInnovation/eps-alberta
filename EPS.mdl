	GET DIRECT CONSTANTS(
	   'InputData/elec/ARpUIiRC/ARpUIiRC.csv',
	   ',',
	   'B2*'
	)
	~	MW/($/(MW*hour))
	~		|

BAU RPS Qualifying Share of Surviving Potential Electricity Output=
	ZIDZ(
	  SUM(BAU Surviving Expected Electricity Output[Electricity Source!,Power Plant Quality\
		!]
	    * RQSD BAU RPS Qualifying Source Definitions[Electricity Source!]),
	
	  SUM(BAU Surviving Expected Electricity Output[Electricity Source!,Power Plant Quality\
		!])
	)
	~	Dmnl
	~		|

BAU Percentage of RPS Qualifying Potential Output to Seek This Year=
	IF THEN ELSE(
	
	  VMAX(BAU Slope of Line Approaching Future RPS Percentages by Year[Future Year!]) <\
		 0,
	
	  0,
	
	  BAU RPS Qualifying Share of Surviving Potential Electricity Output
	  + VMAX(BAU Slope of Line Approaching Future RPS Percentages by Year[Future Year!])
	)
	~	Dmnl
	~		|

BAU Potential RPS Qualifying Electricity Output=
	SUM(
	  (BAU Surviving Expected Electricity Output[Electricity Source!,Power Plant Quality\
		!]
	    + BAU New Potential Output from Policy Mandated Capacity[Electricity Source!])
	  * RQSD BAU RPS Qualifying Source Definitions[Electricity Source!]
	)
	~	MW*hour
	~		|

BAU Additional Potential Output Needed to Meet RPS=
	MAX(
	  0,
	
	  (BAU Elec Demand Including TnD Losses
	    * BAU Percentage of RPS Qualifying Potential Output to Seek This Year)
	  - BAU Potential RPS Qualifying Electricity Output
	
	)
	~	MW*hour
	~		|

BAU Slope of Line Approaching Future RPS Percentages by Year[Future Year]=
	IF THEN ELSE(
	  Time > Numerical Future Year Definitions[Future Year],
	
	  -9999,
	
	  (BAU RPS Percentage by Year[Future Year]
	    - BAU RPS Qualifying Share of Surviving Potential Electricity Output)
	  / (Numerical Future Year Definitions[Future Year] - Time + 1) * One Year
	
	)
	~	Dmnl
	~	We use -9999 for slopes of lines approaching points that are not in the \
		future, because we are going to choose the highest slope to any future \
		point, and we don't want any slopes that are not to future points to be \
		selected.  Note that we haven't built for the current model year yet in \
		the calculation flow, so the current model year counts as being in the \
		future.
	|

BAU RPS Percentage by Year[Future Year]=
	BRPSPTY BAU Renewable Portfolio Std Percentage This Year[Future Year] ~~|

BAU New Allocated Output[Electricity Source]=
	BAU New Elec Output Allocated Under RPS[Electricity Source]
	+ BAU New Elec Output Allocated Freely[Electricity Source]
	~	MW*hour
	~		|

Slope of Line Approaching Future RPS Percentages by Year[Future Year]=
	IF THEN ELSE(
	  Time > Numerical Future Year Definitions[Future Year],
	
	  -9999,
	
	  (RPS Percentage by Year[Future Year]
	    - RPS Qualifying Share of Surviving Potential Electricity Output)
	  / (Numerical Future Year Definitions[Future Year] - Time + 1) * One Year
	
	)
	~	Dmnl
	~	We use -9999 for slopes of lines approaching points that are not in the \
		future, because we are going to choose the highest slope to any future \
		point, and we don't want any slopes that are not to future points to be \
		selected.  Note that we haven't built for the current model year yet in \
		the calculation flow, so the current model year counts as being in the \
		future.
	|

Change in Relative Cost by Source vs BAU[Electricity Source,Power Plant Quality]=
	Difference in Cost Relative to Mean by Source[Electricity Source,Power Plant Quality\
		]
	- BAU Difference in Cost Relative to Mean by Source[Electricity Source,Power Plant Quality\
		]
	~	$/(MW*hour)
	~		|

BAU New Allocated Capacity[Electricity Source]=
	BAU New Allocated Output[Electricity Source]
	/ BAU Expected Capacity Factor x Hours per Year[Electricity Source,newly built]
	~	MW
	~		|

Percentage of RPS Qualifying Potential Output to Seek This Year=
	IF THEN ELSE(
	
	  VMAX(Slope of Line Approaching Future RPS Percentages by Year[Future Year!]) < 0,
	
	  0,
	
	  RPS Qualifying Share of Surviving Potential Electricity Output
	  + VMAX(Slope of Line Approaching Future RPS Percentages by Year[Future Year!])
	)
	~	Dmnl
	~		|

BRPSPTY BAU Renewable Portfolio Std Percentage This Year[Future Year]=
	GET DIRECT CONSTANTS(
	   'InputData/elec/BRPSPTY/BRPSPTY.csv',
	   ',',
	   'B2'
